{
  "name": "parrot-it",
  "version": "0.0.0",
  "description": "Paste Text into a running call using the Nokia MN API",
  "scripts": {
    "build": "tsc --watch",
    "buildOnce": "tsc",
    "start": "node dist/index.js",
    "lint": "tslint --format stylish --project .",
    "prettier": "prettier --config node_modules/tslint-lt/.prettierrc.yml --write \"{,!node_modules/}**/*.{ts,tsx}\"",
    "test": "npm run buildOnce && npm run lint && npm run prettier"
  },
  "files": [
    "src"
  ],
  "main": "src/index",
  "repository": {
    "type": "git",
    "url": "https://github.com/junctionx-nbsp/parrot-it.git"
  },
  "author": "ltetzlaff",
  "license": "All rights reserved.",
  "dependencies": {
    "clipboardy": "^1.2.3",
    "dotenv": "^6.1.0",
    "improved": "^1.5.4",
    "proton-native": "^1.1.9",
    "ws": "^6.1.0"
  },
  "devDependencies": {
    "@types/clipboardy": "^1.1.0",
    "@types/dotenv": "^4.0.3",
    "@types/node": "^10.12.0",
    "@types/proton-native": "^1.1.3",
    "@types/ws": "^6.0.1",
    "tslint-lt": "^1.9.0",
    "typescript": "^3.1.1"
  }
}
